  left: 70px;
  top: 0;
  height: 65px;
  width: 65px;
}
.AdBanner-date {
  color: #273b47;
  position: absolute;
  vertical-align: top;
  top: -10px;
  left: 15px;
}
.AdBanner-dateDay {
  color: #273b47;
  border-bottom: 1px solid;
  display: block;
  text-align: center;
  font-size: 48px;
  font-weight: bold;
}
.AdBanner-dateMonth {
  padding: 5px 2px 0;
  display: block;
  font-size: 16px;
  letter-spacing: 2px;
  text-align: center;
}
.AdBanner-label {
  background: #ff4842;
  color: #fff;
  display: inline-block;
  letter-spacing: 2px;
  font-size: 14px;
  padding: 6px 10px;
  text-transform: uppercase;
}
.AdBanner-title {
  color: #273b47;
  font-size: 26px;
  margin: 0;
}
@media only screen and (max-width: 480px) {
  .AdBanner-content:before {
    content: none;
  }
  .AdBanner-title {
    font-size: 20px;
  }
}
* {
  margin: 0;
}
a {
  color: #0791e6;
}
body {
  font-family: 'Lato', sans-serif;
  font-size: 16px;
  color: #273b47;
  text-rendering: optimizeLegibility;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
